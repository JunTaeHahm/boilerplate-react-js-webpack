{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["ES6", "DOM"],
    "module": "esnext",
    "sourceMap": true,
    "moduleResolution": "Node",
    "target": "es5",
    "forceConsistentCasingInFileNames": false,
    "strict": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@reducers/*": ["./src/reducers/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
}
